    % Predicate for bitwise XOR (n variables)
    predicate int2bin(var 0..255: x, array[0..7] of var bool: bits) =
        (bits[0] = (x div 1) mod 2) /\
        (bits[1] = (x div 2) mod 2) /\
        (bits[2] = (x div 4) mod 2) /\
        (bits[3] = (x div 8) mod 2) /\
        (bits[4] = (x div 16) mod 2) /\
        (bits[5] = (x div 32) mod 2) /\
        (bits[6] = (x div 64) mod 2) /\
        (bits[7] = (x div 128) mod 2);


    
predicate bit_xor3(var 0..255: x0, var 0..255: x1, var 0..255: x2) =
    let { 
        array[0..7] of var bool: x0_bits,
        array[0..7] of var bool: x1_bits,
        array[0..7] of var bool: x2_bits
    } in (
        int2bin(x0, x0_bits) /\ 
        int2bin(x1, x1_bits) /\ 
        int2bin(x2, x2_bits) /\ 
        forall(i in 0..7)(
            ( x0_bits[i] + x1_bits[i] + x2_bits[i]) mod 2 = 0
        )
    );

predicate bit_xor4(var 0..255: x0, var 0..255: x1, var 0..255: x2, var 0..255: x3) =
    let { 
        array[0..7] of var bool: x0_bits,
        array[0..7] of var bool: x1_bits,
        array[0..7] of var bool: x2_bits,
        array[0..7] of var bool: x3_bits
    } in (
        int2bin(x0, x0_bits) /\ 
        int2bin(x1, x1_bits) /\ 
        int2bin(x2, x2_bits) /\ 
        int2bin(x3, x3_bits) /\ 
        forall(i in 0..7)(
            ( x0_bits[i] + x1_bits[i] + x2_bits[i] + x3_bits[i]) mod 2 = 0
        )
    );
